{
  "name": "ai-note",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "electron": "wait-on http://localhost:5173 && electron .",
    "electron:dev": "concurrently \"pnpm run dev\" \"pnpm run electron\"",
    "electron:build": "pnpm run build && electron-builder",
    "electron:dist": "pnpm run build && electron-builder --publish=never"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "axios": "^1.4.0",
    "date-fns": "^4.1.0",
    "element-plus": "^2.3.8",
    "marked": "^16.2.0",
    "pinia": "^2.1.4",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "main": "electron/main.js",
  "homepage": "./",
  "devDependencies": {
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^18.16.19",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "concurrently": "^8.2.0",
    "electron": "^25.3.1",
    "electron-builder": "^24.4.0",
    "eslint": "^8.44.0",
    "eslint-plugin-vue": "^9.15.1",
    "jiti": "^1.18.2",
    "npm-run-all2": "^6.0.6",
    "sass": "^1.91.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.0",
    "vue-tsc": "^1.8.5",
    "wait-on": "^7.0.1"
  }
}
